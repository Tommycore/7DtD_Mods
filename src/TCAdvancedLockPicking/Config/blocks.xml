<?xml version="1.0" encoding="utf-8" ?>

<config>

  <append xpath="/blocks/block/property[@name='Class' and @value='DoorSecure']/..">
    <property class="CompositeFeatures">
      <property class="TEFeatureOpenable">
        <property name="ChanceToStartOpen" value="0.5" />
        <property name="ChanceToStartLocked" value="0.5" />
      </property>
      <property class="TEFeatureLockableTC">
        <property name="LockDifficulty" value="1" />
      </property>
      <property class="TEFeatureLockPickableTC" />
    </property>
  </append>

  <setattribute xpath="/blocks/block/property[@name='Class' and @value='DoorSecure']" name="value">CompositeTileEntity</setattribute>

</config>
